  <pqevent object="pqClientMainWindow/propertiesDock/propertiesPanel/scrollArea/qt_scrollarea_viewport/scrollAreaWidgetContents/DisplayFrame/ProxyPanel/EdgeOpacity/DoubleRangeWidget/DoubleLineEdit" command="key" arguments="16777220" />
