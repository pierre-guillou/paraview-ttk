  <ProxyGroup name="sources">
   <SourceProxy name="ParametricSurfaces" class="vtkParametricFunctionSource"
                label="Parametric Surfaces">
      <IntVectorProperty name="U Resolution" command="SetUResolution"
         number_of_elements="1" default_values="50">
      </IntVectorProperty>
      <IntVectorProperty name="V Resolution" command="SetVResolution"
         number_of_elements="1" default_values="50">
      </IntVectorProperty>
      <IntVectorProperty
        name="Scalar Mode"
        command="SetScalarMode"
        number_of_elements="1"
        default_values="0">
        <EnumerationDomain name="enum">
          <Entry value="0" text="None"/>
          <Entry value="1" text="U"/>
          <Entry value="2" text="V"/>
          <Entry value="3" text="U0"/>
          <Entry value="4" text="V0"/>
          <Entry value="5" text="U0V0"/>
          <Entry value="6" text="Modulus"/>
          <Entry value="7" text="Phase"/>
          <Entry value="8" text="Quadrant"/>
          <Entry value="9" text="X"/>
          <Entry value="10" text="Y"/>
          <Entry value="11" text="Z"/>
        </EnumerationDomain>
        <Documentation>
          This property indicates which scalar mode will be generated.
        </Documentation>
      </IntVectorProperty>
      <IntVectorProperty name="GenerateTextureCoordinates"
                         command="SetGenerateTextureCoordinates"
                         number_of_elements="1"
                         default_values="1">
        <BooleanDomain name="bool"/>
        <Documentation>
          This property whether to generate texture coordinates or not.
        </Documentation>
      </IntVectorProperty>
      <ProxyProperty
       command="SetParametricFunction"
       name="ParametricFunction"
       label="Parametric Surfaces">
       <ProxyListDomain name="proxy_list">
          <Group name="parametric_functions"/>
       </ProxyListDomain>
     </ProxyProperty>
   </SourceProxy>
 </ProxyGroup>
</ServerManagerConfiguration>
